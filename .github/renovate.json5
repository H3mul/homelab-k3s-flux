{
    "autodiscover": false,
    "dryRun": null,
    "printConfig": false,
    "dependencyDashboardAutoclose": true,
    "ignoreTests": true,
    "platform": "github",
    "repositories": ["H3mul/homelab-k3s-flux"],
    "baseBranches": ["main"],
    "ignorePaths": ["disabled/**"],
    "extends": [
        "config:best-practices",
        ":automergeMinor",
        ":automergePr",
        ":disableRateLimiting",
        ":docker"
    ],
    "flux": {
        "fileMatch": ["\\.yaml$"]
    },
    "helm-values": {
        "fileMatch": ["\\.yaml$"]
    },
    "docker-compose": {
        "fileMatch": ["\\.yaml$"]
    },
}
